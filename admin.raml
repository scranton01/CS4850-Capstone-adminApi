              application/json:
                example: |
                  {
                    "groups": [
                      {    
                        "groupid":1,
                        "devices": [
                          {    
                            "deviceid":1,
                            "ownerid":1,
                            "name":"resident1"
                            ]
                          }
                        ]
                      }
                    ]
                  } 
          400:
            body:
              application/json:
                example: |
                  {
                    "status":"specified id doesn't exist"
                  } 
    delete:
        description: delete device information
        responses:
          200:
            body:
              application/json:
                example: |
