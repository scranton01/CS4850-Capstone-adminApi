          200:
            body:
              application/json:
                example: |
                  {
                    "channelid":1,
                    "status":"update was successful"
                  }
          400:
            body:  
              application/json:
                example: |
                  {
                    "status":"specified id doesn't exist"
                  } 
    delete:
        description: delete channel information
        responses:
          200:
            body:
              application/json:
                example: |
                  {
                    "id":1,
                    "status":"delete was successful"
                  }
          400:
            body:
              application/json:
                example: |
