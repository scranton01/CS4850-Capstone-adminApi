                    "status":"delete was successful"
                  }
  /{version}/groups/{groupid}/channels:
    uriParameters:
      version:
        type: number
        description: version of current API
        example: 1
      groupid:
        type: number
        description: group id
        example: 1
        maximum: 999999999
        minimum: 1
    post:
        description: create new channels id
        responses:
          201:
            body:
              application/json:
                example: |
