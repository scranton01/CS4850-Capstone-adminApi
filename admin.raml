                example: |
                  {
                    "groups": [
                      {    
                        "groupid":1,
                        "channels":[
                          {
                            "channelid":1,
                            "name":"weather",
                            "title":"Weather",
                            "secure":"true",
                            "text":"Marietta, GA",
                            "refresh_time":50
                          }
                        ]
                      }
                    ]
                  } 
          400:
            body:
              application/json:
                example: |
                  {
                    "status":"specified id doesn't exist"
                  } 
    put:
