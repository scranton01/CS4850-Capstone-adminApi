                    "id":1,
                    "status":"Create was successful."
                  }  
    get:
        description: get channel information for specified id
        responses:
          200:
            body:
              application/json:
                example: |
                  {
                    "groups": [
                      {    
                        "groupid":1,
                        "channels":[
                          {
                            "channelid":1,
                            "name":"weather",
                            "title":"Weather",
                            "secure":"true",
                            "text":"Marietta, GA",
                            "refresh_time":50
                          }
                        ]
                      }
                    ]
                  }   
          400:
            body:
              application/json:
                example: |
                  {
                    "status":"specified id doesn't exist"
                  } 
  /{version}/groups/{groupid}//channels/{channelid}:
    uriParameters:
      version:
        type: number
        description: version of current API
        example: 1
      groupid:
        type: number
        description: group id
        example: 1
        maximum: 999999999
        minimum: 1
      channelid:
        type: number
        description: channel id
        example: 1
        maximum: 999999999
        minimum: 1
    get:
        description: get channel information for specified id
